<file path=".github/workflows/deploy-prod.yml">
name: Deploy to Production

on:
  push:
    branches:
      - main
      - master
    tags:
      - 'v*'
  workflow_dispatch:

jobs:
  deploy:
    runs-on: ubuntu-latest
    environment: production

    steps:
    - name: Checkout code
      uses: actions/checkout@v4

    - name: Set up Python
      uses: actions/setup-python@v5
      with:
        python-version: '3.12'

    - name: Install dependencies
      run: |
        python -m pip install --upgrade pip
        pip install -r requirements.txt

    - name: Run tests before deploy to production
      env:
        DJANGO_SETTINGS_MODULE: config.settings.dev
      run: |
        cd mysite
        python manage.py check
        python manage.py test --noinput

    - name: Security check before production
      run: |
        bandit -r mysite/ -ll
        safety check
      continue-on-error: false

    - name: Setup SSH
      uses: webfactory/ssh-agent@v0.9.0
      with:
        ssh-private-key: ${{ secrets.SSH_PRIVATE_KEY }}

    - name: Add server to known hosts
      run: |
        ssh-keyscan -p ${{ vars.SSH_PORT }} ${{ vars.SSH_HOST }} >> ~/.ssh/known_hosts

    - name: Create backup before deploy
      env:
        SSH_HOST: ${{ vars.SSH_HOST }}
        SSH_USER: ${{ vars.SSH_USER }}
        SSH_PORT: ${{ vars.SSH_PORT }}
        DEPLOY_PATH: ${{ vars.DEPLOY_PATH_PROD }}
