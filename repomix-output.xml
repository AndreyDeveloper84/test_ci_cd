<file path="scripts/deploy.sh">
#!/bin/bash

# –°–∫—Ä–∏–ø—Ç –¥–µ–ø–ª–æ—è Django –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è –Ω–∞ —Å–µ—Ä–≤–µ—Ä
# –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ: ./deploy.sh [stage|prod]

set -e  # –û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –ø—Ä–∏ –æ—à–∏–±–∫–µ

ENVIRONMENT=$1

if [ -z "$ENVIRONMENT" ]; then
    echo "‚ùå –û—à–∏–±–∫–∞: –ù–µ —É–∫–∞–∑–∞–Ω–æ –æ–∫—Ä—É–∂–µ–Ω–∏–µ (stage –∏–ª–∏ prod)"
    echo "–ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ: ./deploy.sh [stage|prod]"
    exit 1
fi

if [ "$ENVIRONMENT" != "stage" ] && [ "$ENVIRONMENT" != "prod" ]; then
    echo "‚ùå –û—à–∏–±–∫–∞: –û–∫—Ä—É–∂–µ–Ω–∏–µ –¥–æ–ª–∂–Ω–æ –±—ã—Ç—å 'stage' –∏–ª–∏ 'prod'"
    exit 1
fi

echo "üöÄ –ù–∞—á–∞–ª–æ –¥–µ–ø–ª–æ—è –≤ –æ–∫—Ä—É–∂–µ–Ω–∏–µ: $ENVIRONMENT"
echo "üìÖ –î–∞—Ç–∞: $(date '+%Y-%m-%d %H:%M:%S')"

# –û–ø—Ä–µ–¥–µ–ª—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –æ–∫—Ä—É–∂–µ–Ω–∏—è
if [ "$ENVIRONMENT" == "stage" ]; then
    SETTINGS_MODULE="config.settings.stage"
    SERVICE_NAME="mysite-stage"
else
    SETTINGS_MODULE="config.settings.prod"
    SERVICE_NAME="mysite-prod"
fi

echo "‚öôÔ∏è  –ù–∞—Å—Ç—Ä–æ–π–∫–∏:"
echo "  - –û–∫—Ä—É–∂–µ–Ω–∏–µ: $ENVIRONMENT"
echo "  - Settings module: $SETTINGS_MODULE"
echo "  - Service name: $SERVICE_NAME"
echo "  - Deploy path: $DEPLOY_PATH"

# –°–æ–∑–¥–∞–µ–º –≤—Ä–µ–º–µ–Ω–Ω—ã–π —Ñ–∞–π–ª —Å –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–º–∏ –æ–∫—Ä—É–∂–µ–Ω–∏—è
ENV_FILE=$(mktemp)
cat > "$ENV_FILE" <<EOF
DJANGO_SETTINGS_MODULE=${DJANGO_SETTINGS_MODULE}
DJANGO_SECRET_KEY='${DJANGO_SECRET_KEY}'
DB_NAME=${DB_NAME}
DB_USER=${DB_USER}
DB_PASSWORD='${DB_PASSWORD}'
DB_HOST=${DB_HOST}
DB_PORT=${DB_PORT}
DJANGO_ALLOWED_HOSTS='${DJANGO_ALLOWED_HOSTS}'
EOF

echo "üì¶ –°–æ–∑–¥–∞–Ω–∏–µ –∞—Ä—Ö–∏–≤–∞ –ø—Ä–æ–µ–∫—Ç–∞..."

# –°–æ–∑–¥–∞–µ–º –∞—Ä—Ö–∏–≤ –ø—Ä–æ–µ–∫—Ç–∞ (–∏—Å–∫–ª—é—á–∞—è –Ω–µ–Ω—É–∂–Ω—ã–µ —Ñ–∞–π–ª—ã)
tar --exclude='.git' \
    --exclude='.venv' \
    --exclude='__pycache__' \
    --exclude='*.pyc' \
    --exclude='*.pyo' \
    --exclude='db.sqlite3' \
    --exclude='.env' \
    --exclude='staticfiles' \
    --exclude='media' \
    --exclude='logs' \
    --exclude='*.log' \
    --exclude='.pytest_cache' \
    --exclude='htmlcov' \
    --exclude='backups' \
    -czf /tmp/mysite-deploy.tar.gz .

echo "üì§ –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ —Ñ–∞–π–ª–æ–≤ –Ω–∞ —Å–µ—Ä–≤–µ—Ä..."

# –ö–æ–ø–∏—Ä—É–µ–º –∞—Ä—Ö–∏–≤ –Ω–∞ —Å–µ—Ä–≤–µ—Ä
scp -P "$SSH_PORT" /tmp/mysite-deploy.tar.gz "$SSH_USER@$SSH_HOST:/tmp/"

# –ö–æ–ø–∏—Ä—É–µ–º —Ñ–∞–π–ª —Å –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–º–∏ –æ–∫—Ä—É–∂–µ–Ω–∏—è
